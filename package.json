{
  "name": "three-edit-tools",
  "version": "1.2.0",
  "description": "A powerful, modular 3D editing library built on Three.js with a comprehensive set of tools for mesh manipulation, geometry operations, and 3D content creation.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "build:umd": "vite build --config vite.umd.config.ts",
    "build:browser": "vite build --config vite.browser.config.ts",
    "build:all": "npm run build && npm run build:umd && npm run build:browser",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.ts\"",
    "demo": "vite",
    "demo:build": "vite build",
    "demo:preview": "vite preview"
  },
  "keywords": [
    "three.js",
    "3d",
    "editor",
    "modeling",
    "geometry",
    "mesh"
  ],
  "author": "three-edit contributors",
  "peerDependencies": {
    "three": ">=0.155.0"
  },
  "devDependencies": {
    "@types/three": "^0.169.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.5.3",
    "http-server": "^14.1.1",
    "prettier": "^3.3.3",
    "terser": "^5.43.1",
    "three": "^0.169.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^5.3.4",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^2.0.5",
    "tsup": "^8.1.0"
  },
  "dependencies": {
    "ajv": "^8.17.1"
  }
}
